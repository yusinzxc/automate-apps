
alias autodl="python ./Code-Folder/PY/autoYt-dl/autodl.py"
alias rps="python ./Code-Folder/PY/RockPaperScissor/start.py"
alias apps="cat .apps"

#WEBSITES

#Github
alias github="xdg-open https://github.com/yusinzxc?tab=repositories && clear && cat .apps"

#Virustotal
alias virustotal="am start org.chromium.webapk.a66194eb388fc3285_v2/org.chromium.webapk.shell_apk.h2o.H2OOpaqueMainActivity && clear && cat .apps"


#APPS

#Youtube
alias yt="xdg-open https://youtube.com"

#File Manager
alias files="am start com.coloros.filemanager/com.coloros.filemanager.filerefactor.ui.main.MainActivity && clear && cat .apps"

#Chrome
alias chrome="am start com.android.chrome/com.google.android.apps.chrome.Main && clear && cat .apps"

#Firefox
alias firefox="am start org.mozilla.firefox/org.mozilla.firefox.App && clear && cat .apps"

#Gallery
alias gallery="am start com.coloros.gallery3d/com.oppo.gallery3d.app.Gallery && clear && cat .apps"

#Gmail
alias gmail="am start com.google.android.gm/com.android.mail.ui.MailActivity && clear && cat .apps"

#Facebook
alias fblite="am start com.facebook.lite/com.facebook.lite.MainActivity && clear && cat .apps"

#Newpipe
